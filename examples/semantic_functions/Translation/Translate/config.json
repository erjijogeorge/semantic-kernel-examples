{
  "schema": 1,
  "description": "Translates text to a target language",
  "execution_settings": {
    "default": {
      "max_tokens": 500,
      "temperature": 0.3,
      "top_p": 0.9
    }
  },
  "input_variables": [
    {
      "name": "input",
      "description": "The text to translate",
      "required": true
    },
    {
      "name": "target_language",
      "description": "The language to translate to",
      "default": "Spanish"
    }
  ]
}
